.DocsHeader,
.DocsAside {
    left: 0;
    position: fixed;
    top: 0;
    width: 280px;
}

.DocsAside {
    height: calc(100% - 46px);
    overflow: scroll;
    top: 46px;
}

.DocsNav__MobileActivator {
    display: none;
}

.DocsNav__Sections {
    display: block;
}

.DocsContent {
    padding-left: 280px;
}
